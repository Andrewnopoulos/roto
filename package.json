{
  "name": "roto-multiplayer-game",
  "version": "1.0.0",
  "description": "A real-time multiplayer tic-tac-toe game built with Node.js, Express, and Socket.io",
  "main": "server.js",
  "scripts": {
    "start": "node setup-railway-db.js && node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "docker:build": "docker build -t roto-game .",
    "docker:run": "docker run -p 3000:3000 roto-game",
    "docker:compose": "docker-compose up -d",
    "docker:compose:prod": "docker-compose --profile production up -d",
    "docker:stop": "docker-compose down",
    "railway:build": "echo 'Build completed successfully'",
    "railway:setup": "node railway-db-setup.js",
    "railway:deploy": "node railway-db-setup.js && node server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "pg": "^8.16.3",
    "rate-limiter-flexible": "^2.4.2",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "game",
    "multiplayer",
    "tic-tac-toe",
    "socket.io",
    "nodejs",
    "express",
    "real-time",
    "websocket"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/roto.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/roto/issues"
  },
  "homepage": "https://github.com/yourusername/roto#readme"
}
